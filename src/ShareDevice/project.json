  //},

  "runtimes": {
    "win7-x86": {},
    "win7-x64": {},
    "win10-x64": {},
    "osx.10.10-x64": {},
    "osx.10.11-x64": {},
    "osx.10.12-x64": {},
    "centos.7-x64": {}
  }
}
